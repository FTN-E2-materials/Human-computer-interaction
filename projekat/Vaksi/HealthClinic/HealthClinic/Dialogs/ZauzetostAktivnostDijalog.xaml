        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:HealthClinic.Dialogs"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Zauzetost/Aktivnost" Height="450" Width="400"
        MaxHeight="450" MinHeight="450"
        MaxWidth="400" MinWidth="400">
    <Grid Background="#cdfbf4">

        <!-- Text iznad aktivnosti-->
        <TextBlock 
                HorizontalAlignment="Left"
                FontFamily="Times New Roman" 
                FontSize="22" 
                FontStyle="Italic"
                Text="Trenutna zauzetost i aktivnost prostorije"
                Margin="20, 20, 0, 0"
                Foreground="#FF2196F3">
        </TextBlock>
        <!-- Kraj teksta iznad aktivnosti -->

    </Grid>
</Window>
